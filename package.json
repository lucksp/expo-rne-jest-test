{
  "main": "node_modules/expo/AppEntry.js",
  "name": "lieutenant_app",
  "version": "1.6.4",
  "private": true,
  "prettier": "@fluidtruck/prettier",
  "scripts": {
    "clean": "rm -rf .expo && rm -rf node_modules && yarn",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=19000'",
    "debug.eas": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "eas:build.dev.sim": "eas build --profile development --platform ios",
    "eas:build.dev.device": "eas build --profile development-device --platform ios",
    "eas:build.stage.test": "eas build --profile test-flight:stage --platform ios",
    "eas:build.prod": "eas build --profile test-flight --platform ios",
    "eas:build.prod.internal": "eas build --profile prod-internal --platform ios",
    "eas:dev": "NODE_ENV=development npx expo start --dev-client",
    "eject": "expo eject",
    "expo:install": "npx expo install",
    "ios": "expo start --ios",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx --cache --cache-location .eslintcache --report-unused-disable-directives",
    "prettier": "prettier --write",
    "start": "expo start --dev-client",
    "test": "node_modules/.bin/jest",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand",
    "test:watch": "jest --watch",
    "validate": "yarn tsc && yarn lint && yarn ts-unused-exports tsconfig.json"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/netinfo": "9.3.0",
    "@react-navigation/core": "^6.2.2",
    "@react-navigation/drawer": "^6.6.2",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.9.12",
    "@react-navigation/stack": "^5.14.4",
    "@rneui/base": "github:react-native-elements/react-native-elements#base",
    "@rneui/themed": "github:react-native-elements/react-native-elements#themed",
    "base-64": "^1.0.0",
    "dotenv": "^16.0.0",
    "expo": "~46.0.19",
    "expo-constants": "~13.2.4",
    "expo-dev-client": "~1.3.1",
    "expo-linking": "~3.2.4",
    "expo-location": "~14.3.0",
    "expo-secure-store": "~11.3.0",
    "immer": "^9.0.16",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.6",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "swr": "^1.3.0",
    "use-immer": "^0.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/preset-typescript": "^7.16.7",
    "@jest/create-cache-key-function": "^29.5.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-native": "^12.0.0",
    "@types/base-64": "^1.0.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.14",
    "@types/react-native-dotenv": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "cross-fetch": "^3.1.5",
    "eslint": "^8.8.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27.4.7",
    "jest-expo": "^44.0.1",
    "msw": "^0.47.2",
    "prettier": "^2.7.1",
    "react-test-renderer": "18.0.0",
    "ts-node": "^10.4.0",
    "ts-unused-exports": "^7.0.3",
    "typescript": "^4.5.5"
  },
  "resolutions": {
    "@jest/create-cache-key-function": "^27.0.2"
  }
}
